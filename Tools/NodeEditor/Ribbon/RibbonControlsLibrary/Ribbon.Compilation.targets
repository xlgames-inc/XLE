  <PropertyGroup>
    <!-- OutputPath controls where *.dll.lce files are dropped to and must be backslash-terminated.
         BinplaceRoot controls where *.dll files are dropped to and does not need to be backslash-terminated.
         It is necessary to define these properties here after importing targets to override imported values. -->
    <OutputPath>$(OutputPath)\$(TargetFrameworkVersion)\</OutputPath>
    <BinplaceRoot>$(OutputPath)</BinplaceRoot>
  </PropertyGroup>

  <!-- The 3.5 and 4.0 SxS builds stomp over the copy of RibbonControlsLibrary.pdb under Symbols.pri.
       Caching the PDBs in a SxS subfolder allows them to both exist after the build, which is what setup needs. -->
  <Target Name="_SxSPlaceSymbols"
          AfterTargets="PlaceSymbols">
    <PropertyGroup>
      <Origin>$(IntermediateOutputPath)$(TargetName).pdb</Origin>
      <Destination>$(_NTTREE)\wpf\$(TargetFrameworkVersion)\$(TargetName).pdb</Destination>
    </PropertyGroup>

    <Copy SourceFiles="$(Origin)"
          DestinationFiles="$(Destination)"/>

    <Message Text="Successfully copied $(Origin) to $(Destination)."/>
  </Target>

